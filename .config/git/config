
[core]
    autocrlf = false

[alias]
	tim = log --all --color --decorate --graph --pretty=format:'%Cred%h%Creset -%C(yellow)%d%Creset %s %Cgreen(%cr) %C(bold blue)<%an>%Creset' --abbrev-commit
	up = checkout
	ci = commit -a
	me = merge --no-ff --no-commit
	cle = clean -d -f
	rev = restore .
	sum = status

